{
  "compilerOptions": {
    "outDir": "dist",
    "target": "ES5",
    "lib": ["ESNext"],
    "skipLibCheck": true,
    "esModuleInterop": false,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "Node16",
    "moduleResolution": "Node16",
    "resolveJsonModule": true,
    "baseUrl": ".",
    "typeRoots": ["./node_modules/@types"]
  },
  "include": ["src/server.ts"]
}
